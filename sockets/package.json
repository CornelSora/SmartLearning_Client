{
    "name": "socket-compiler",
    "version": "1.0.0",
    "description": "Socket server to execute code",
    "scripts": {
        "start:build": "node dist",
        "prestart": "npm run build",
        "lint": "eslint src test; exit 0",
        "lint:fix": "eslint src test --fix; exit 0",
        "codecov": "cat coverage/*/lcov.info | codecov",
        "build": "rimraf dist && babel src -D --out-dir dist",
        "test": "NODE_ENV=test mocha --recursive --require babel-register test",
        "prettify": "prettier --single-quote --print-width 120 --write \"src/**/*.js\" \"test/**/*.js\"",
        "start": "nodemon src",
        "test:coverage": "NODE_ENV=test nyc --require babel-register --reporter lcov --reporter text mocha --recursive test",
        "debug": "node --nolazy --inspect-brk=8081 src/index.js"
    },
    "dependencies": {
        "codemirror": "^5.40.0",
        "crypto-js": "^3.1.9-1",
        "docker-cli-js": "^2.5.2",
        "eslint": "^4.7.0",
        "eslint-config-prettier": "^2.6.0",
        "eslint-plugin-prettier": "^2.3.1",
        "express": "^4.16.3",
        "jsonwebtoken": "^8.5.1",
        "node-cmd": "^3.0.0",
        "node-powershell": "^4.0.0",
        "nodemon": "^1.11.0",
        "nyc": "^11.4.1",
        "socket.io": "^2.1.1",
        "winston": "^3.0.0",
        "winston-daily-rotate-file": "^3.3.0"
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.24.1",
        "codecov": "^2.2.0",
        "eslint": "^4.7.0",
        "eslint-config-prettier": "^2.6.0",
        "eslint-plugin-prettier": "^2.3.1",
        "husky": "^0.14.3",
        "lint-staged": "^4.2.1",
        "nodemon": "^1.11.0",
        "nyc": "^11.4.1",
        "prettier": "^1.7.4"
    },
    "engines": {
        "node": ">= 6.9.0",
        "npm": ">= 3.10.8"
    }
}
